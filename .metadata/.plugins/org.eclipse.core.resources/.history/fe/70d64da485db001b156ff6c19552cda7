        Pattern p = Pattern.compile("^\\w+@\\w+\\.\\w{3}");
        Matcher m;
        int n = in.nextInt();
        in.nextLine();
        String str = "";
        List<String> matches = new ArrayList<>();
        for(int i = 0; i < n; i++)
        {
        	str += in.nextLine() + " ";
        }
    }
}